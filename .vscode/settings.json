{
    "editor.tabSize": 4,
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/*.js": {
            "when": "$(basename).ts"
        },
        "**/*.js.map": true,
        "out": false
    },
    "files.trimTrailingWhitespace": true,
    "search.exclude": {
        "out": true
    }
}
